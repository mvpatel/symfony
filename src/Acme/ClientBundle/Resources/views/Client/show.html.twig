{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Client</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ entity.firstName }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ entity.lastName }}</td>
            </tr>
            <tr>
                <th>Clientemail</th>
                <td>{{ entity.clientEmail }}</td>
            </tr>
            <tr>
                <th>Clientmobile</th>
                <td>{{ entity.clientMobile }}</td>
            </tr>
            <tr>
                <th>Clientgender</th>
                <td>{{ entity.clientGender }}</td>
            </tr>
            <tr>
                <th>Clientcar</th>
                <td>{{ entity.clientCar }}</td>
            </tr>
            <tr>
                <th>Clientcarname</th>
                <td>{{ entity.clientCarName }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('client') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('client_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
