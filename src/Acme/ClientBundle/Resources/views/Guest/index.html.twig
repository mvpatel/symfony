{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Guest list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Guestfirstname</th>
                <th>Guestlastname</th>
                <th>Gusetgender</th>
                <th>Guestcar</th>
                <th>Gusetcarname</th>
                <th>Guestcarcompany</th>
                <th>Gusetcompany</th>
                <th>Guestcompanyname</th>
                <th>Guestmobileno</th>
                <th>Guestphoneno</th>
                <th>Gusetregisterdate</th>
                <th>Guestloginname</th>
                <th>Guestlogindate</th>
                <th>Guestpassword</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('guest_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.guestFirstName }}</td>
                <td>{{ entity.guestLastName }}</td>
                <td>{{ entity.gusetGender }}</td>
                <td>{{ entity.guestCar }}</td>
                <td>{{ entity.gusetCarName }}</td>
                <td>{{ entity.guestCarCompany }}</td>
                <td>{{ entity.gusetCompany }}</td>
                <td>{{ entity.guestCompanyName }}</td>
                <td>{{ entity.guestMobileNo }}</td>
                <td>{{ entity.guestPhoneNo }}</td>
                <td>{{ entity.gusetRegisterDate }}</td>
                <td>{{ entity.guestLoginName }}</td>
                <td>{{ entity.guestLoginDate }}</td>
                <td>{{ entity.guestPassword }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('guest_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('guest_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('guest_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
